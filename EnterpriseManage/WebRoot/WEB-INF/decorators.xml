<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/jsp">
	<!-- Any urls that are excluded will never be decorated by Sitemesh -->
	<excludes>
		<pattern>/js*</pattern>
		<pattern>/styles/*</pattern>
		<pattern>/content/upload/*</pattern>
		<pattern>/management*</pattern>
	</excludes>
	
	<decorator name="www" page="layout/template.jsp">
		<pattern>/*</pattern>
	</decorator>
	
</decorators>
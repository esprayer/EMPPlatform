public class JDRStyleItemEntry {
    /**
     *
     */
    public String id = "";
    public String caption = "";
    public Object userObject = "";
    /**
     *
     */
    public JDRStyleItemEntry(String id, String caption) {
        this.id = id;
        this.caption = caption;
    }

    /**
     *
     * @return
     */
    public String toString() {
        return id + " - " + caption;
    }
}